{
  "name": "learn-with-365",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://365evergreen.github.io/learn-with-365",
  "repository": {
    "type": "git",
    "url": "https://github.com/365Evergreen/learn-with-365.git"
  },
  "bugs": {
    "url": "https://github.com/365Evergreen/learn-with-365/issues"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.25.0",
    "@azure/msal-react": "^3.0.20",
    "@fluentui/react-components": "^9.72.2",
    "@fluentui/react-icons": "^2.0.312",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:azure": "pwsh -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target azure",
    "deploy:netlify": "pwsh -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target netlify",
    "deploy:docker": "pwsh -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target docker",
    "deploy:local": "pwsh -ExecutionPolicy Bypass -File scripts/deploy.ps1 -Target local",
    "build:docker": "docker build -t elearning-platform .",
    "start:docker": "docker run -p 3000:80 elearning-platform",
    "dev:docker": "docker-compose --profile dev up",
    "analyze": "npm run build && npx serve -s build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "deploy:gh-pages": "gh-pages -d build",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prebuild": "npm run type-check"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
